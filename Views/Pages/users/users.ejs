<div class="content container-fluid">
  <!-- Page Header -->
  <div class="page-header">
    <div class="row">
      <div class="col-sm-12">
        <div class="page-sub-header">
          <h3 class="page-title">users</h3>
          <ul class="breadcrumb">
            <li class="breadcrumb-item"><a href="userss">users</a></li>
            <li class="breadcrumb-item active">All users</li>
          </ul>
        </div>
      </div>
    </div>
  </div>
  <!-- /Page Header -->

  <div class="users-group-form">
    <div class="row">
      <div class="col-lg-3 col-md-6">
        <div class="form-group">
          <input
            type="text"
            class="form-control"
            placeholder="Search by ID ..."
          />
        </div>
      </div>
      <div class="col-lg-3 col-md-6">
        <div class="form-group">
          <input
            type="text"
            class="form-control"
            placeholder="Search by Name ..."
          />
        </div>
      </div>
      <div class="col-lg-4 col-md-6">
        <div class="form-group">
          <input
            type="text"
            class="form-control"
            placeholder="Search by Phone ..."
          />
        </div>
      </div>
      <div class="col-lg-2">
        <div class="search-users-btn">
          <button type="btn" class="btn btn-primary">Search</button>
        </div>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col-sm-12">
      <div class="card card-table comman-shadow">
        <div class="card-body">
          <!-- Page Header -->
          <div class="page-header">
            <div class="row align-items-center">
              <div class="col">
                <h3 class="page-title">userss</h3>
              </div>
              <div class="col-auto text-end float-end ms-auto download-grp">
                <a href="userss" class="btn btn-outline-gray me-2 active"
                  ><i class="feather-list"></i
                ></a>
                <a href="userss-grid" class="btn btn-outline-gray me-2"
                  ><i class="feather-grid"></i
                ></a>
                <a href="#" class="btn btn-outline-primary me-2"
                  ><i class="fas fa-download"></i> Download</a
                >
                <a href="add-users" class="btn btn-primary"
                  ><i class="fas fa-plus"></i
                ></a>
              </div>
            </div>
          </div>
          <!-- /Page Header -->

          <div class="table-responsive">
            <table
              class="table border-0 star-users table-hover table-center mb-0 datatable table-striped"
            >
              <thead class="users-thread">
                <tr>
                  <th>NIC</th>
                  <th>Name</th>
                  <th>Department</th>
                  <th>Telephone</th>
                  <th>Email</th>
                  <th>Role</th>
                  <!-- <th>Address</th> -->
                  <th class="text-end">Action</th>
                </tr>
              </thead>
              <tbody>
                <%userRes.forEach(user=>{%>
                <tr>
                  <td><%=user.nic%></td>
                  <td><%=user.name%></td>
                  <td><%=user.department%></td>
                  <td><%=user.telephone%></td>
                  <td><%=user.email%></td>
                  <td><%=user.role%></td>
                  <td class="text-end">
                    <div class="actions">
                      <a
                        href="javascript:;"
                        class="btn btn-sm bg-success-light me-2"
                      >
                        <i class="feather-eye"></i>
                      </a>
                      <a href="edit-users" class="btn btn-sm bg-danger-light">
                        <i class="feather-edit"></i>
                      </a>
                    </div>
                  </td>
                </tr>
                <%})%>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
